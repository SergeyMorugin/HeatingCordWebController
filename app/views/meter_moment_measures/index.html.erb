<p id="notice"><%= notice %></p>

<h1>Meter Moment Measures</h1>

<table>
  <thead>
    <tr>
      <th>Voltage</th>
      <th>Amperage</th>
      <th>Power</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meter_moment_measures.each do |meter_moment_measure| %>
      <tr>
        <td><%= meter_moment_measure.voltage %></td>
        <td><%= meter_moment_measure.amperage %></td>
        <td><%= meter_moment_measure.power %></td>
        <td><%= link_to 'Show', meter_moment_measure %></td>
        <td><%= link_to 'Edit', edit_meter_moment_measure_path(meter_moment_measure) %></td>
        <td><%= link_to 'Destroy', meter_moment_measure, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meter Moment Measure', new_meter_moment_measure_path %>
